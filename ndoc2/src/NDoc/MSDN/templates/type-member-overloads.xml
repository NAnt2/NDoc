<?xml version="1.0" ?>
<!DOCTYPE template [
	<!ENTITY copy "&#xA9;">
	<!ENTITY in "   ">
	<!ENTITY nbsp "&#xA0;">
	<!ENTITY nl "&#xD;&#xA;">
]>
<template>
	<html>
		<head>
			<title>
				<member-name />
				<text> Method</text>
			</title>
			<link rel="stylesheet" href="msdn.css" />
			<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		</head>
		<body topmargin="0" id="bodyID" class="dtBODY">
			<div id="nsbanner">
				<div id="bannerrow1">
					<table class="bannerparthead" cellspacing="0">
						<tr id="hdr">
							<!-- TODO: The user should be able to specify these via properties. -->
							<td class="runninghead" nowrap="nowrap">.NET Framework Class Library</td>
							<td class="product" nowrap="nowrap">&nbsp;</td>
						</tr>
					</table>
				</div>
				<div id="TitleRow">
					<h1 class="dtH1">
						<type-name />
						<text>.</text>
						<member-name />
						<text> Method</text>
					</h1>
				</div>
			</div>
			<div id="nstext" valign="bottom">
				<member-overloads-summary />
				<h4 class="dtH4">Overload List</h4>
				<for-each-overloaded-member-in-type>
					<member-summary />
					<blockquote class="dtBlock">
						<span class="lang">[Visual&nbsp;Basic]</span>
						<text> </text>
						<a href="{$member-link}">
							<type-access lang="VB" />
							<text> </text>
							<member-name />
							<text>(</text>
							<for-each-parameter-in-member>
								<parameter-type-name />
								<if-not-last-parameter>
									<text>, </text>
								</if-not-last-parameter>
							</for-each-parameter-in-member>
							<text>)</text>
						</a>
					</blockquote>
					<blockquote class="dtBlock">
						<span class="lang">[C#]</span>
						<text> </text>
						<a href="{$member-link}">
							<type-access lang="C#" />
							<text> </text>
							<member-name />
							<text>(</text>
							<for-each-parameter-in-member>
								<parameter-type-name />
								<if-not-last-parameter>
									<text>, </text>
								</if-not-last-parameter>
							</for-each-parameter-in-member>
							<text>);</text>
						</a>
					</blockquote>
				</for-each-overloaded-member-in-type>
				<h4 class="dtH4">See Also</h4>
				<p>
					<a href="{$type-link}">
						<type-name />
						<text> </text>
						<type-type />
					</a>
					<text> | </text>
					<a href="{$type-members-link}">
						<type-name />
						<text> Members</text>
					</a>
					<text> | </text>
					<a href="{$namespace-link}">
						<namespace-name />
						<text> Namespace</text>
					</a>
				</p>
				<div class="footer">
					<hr />
					<!-- TODO: We should probably let the user specify these via properties. -->
					<p><a href="mailto:ndoc-devel@lists.sourceforge.net">Send comments on this topic.</a></p>
					<p><a href="http://ndoc.sourceforge.net/">&copy; 2002 NDoc Corporation. All rights reserved.</a></p>
				</div>
			</div>
		</body>
	</html>
</template>
